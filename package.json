{
  "name": "stepping",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging --debug",
    "build:uat": "tsc && vite build --mode uat",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "chakra-cli": "chakra-cli",
    "theme:watch": "chakra-cli tokens src/themes/index.ts --watch",
    "theme": "chakra-cli tokens src/themes/index.ts",
    "prettier": "prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write ./src && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && git add -A ."
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "^16.3.0",
    "@hookform/resolvers": "^3.3.0",
    "@loadable/component": "^5.15.3",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-table": "^8.9.3",
    "@tanstack/react-virtual": "^3.0.0-alpha.0",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "libphonenumber-js": "^1.10.44",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.4",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "react-use": "^17.4.0",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.3",
    "chakra-cli": "^1.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
